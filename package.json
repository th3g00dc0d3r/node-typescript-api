  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.14.37",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "jest": "^26.6.3",
